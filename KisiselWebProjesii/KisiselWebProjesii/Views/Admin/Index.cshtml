@using KisiselWebProjesii.Models.Siniflar
@model List<AnaSayfa>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<h2>Ana Sayfa</h2>
<table class="table table-bordered mt-3">
    <thead>
        <tr>
            <th>Id</th>
            <th>Ikon</th>
            <th>Güncelle</th>
            <th>Sil</th>
        </tr>
    

        @foreach (var x in Model)
        {
            <tr>
                <td>@x.id</td>
                <td>@x.isim</td>
                <td><a href="/Admin/AnaSayfaGetir/@x.id" class="btn btn-primary">Güncelle</a></td>
                <td><a href="" class="btn btn-danger">Sil</a></td>
           
            </tr>
        }
     
           </table>

           